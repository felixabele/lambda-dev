{
  "name": "lambda-dev",
  "version": "1.8.22",
  "description": "Develop and Bundle Your Lambda Functions With Ease",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "homepage": "https://github.com/iiroj/lambda-dev",
  "repository": "github:iiroj/lambda-dev",
  "bugs": "https://github.com/iiroj/lambda-dev/issues",
  "bin": "cli.js",
  "files": [
    "cli.js"
  ],
  "engines": {
    "node": ">= 8.10"
  },
  "scripts": {
    "watch": "rollup -cw",
    "lint": "tslint -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "pretest": "rimraf test/tmp",
    "test": "jest",
    "build": "rollup -c",
    "version": "standard-version --no-verify",
    "prepare": "npm run build"
  },
  "lint-staged": {
    "*.{ts}": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run typecheck && npm test && npm run build"
    }
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "express": "^4.17.1",
    "fast-glob": "^3.1.1",
    "make-dir": "^3.0.0",
    "require-from-string": "^2.0.2",
    "webpack": "^4.41.5",
    "webpack-merge": "^4.2.2",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.2.0",
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.25",
    "@types/memory-fs": "^0.3.2",
    "@types/node": "^13.1.1",
    "@types/require-from-string": "^1.2.0",
    "@types/rimraf": "^2.0.3",
    "@types/supertest": "^2.0.8",
    "@types/webpack": "^4.41.0",
    "@types/webpack-merge": "^4.1.5",
    "@types/yargs": "^13.0.4",
    "command-line-test": "^1.0.10",
    "commitlint": "^8.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.0-beta.14",
    "memory-fs": "^0.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.14",
    "rollup-plugin-typescript2": "^0.25.3",
    "standard-version": "^7.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.4"
  },
  "keywords": [
    "babel",
    "dev-server",
    "express",
    "http",
    "lambda",
    "local-server",
    "preset-env",
    "serverless",
    "webpack"
  ]
}
